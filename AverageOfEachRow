public class Main {
  public static void main(String[] args) {
  
  int array[][] = {{1, 2, 3}, {4, 5, 6}};
      for(int i = 0; i< array.length;i++){
      int sum = 0;
        for(int j = 0; j < array[i].length; j++){
           sum = sum +array[i][j];
        } 
        System.out.println("The average of row "+i+" is: "+sum/array[i].length);
      }
  }
}
